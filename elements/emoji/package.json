    "start:dev": "cd ../../ && yarn start emoji --proxy ./packages/elements/emoji/proxy.ts",
    "start:edge": "cd ../../ && PROXY_TARGET_ENV=edge yarn start emoji --proxy ./packages/elements/emoji/proxy.ts"
